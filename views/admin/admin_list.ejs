<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Admins</title>
</head>
<body>
  
    <h1>List of All Admins</h1>
    <div>
      <a href="/books/index" target="_blank" rel="noopener noreferrer">
        <h2>Books Page For Admin</h2>

      </a>
      
    </div>
    <table>
        <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Role</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>
          <% admins.forEach(function(admin) { %>
          <tr>
            <td><%= admin.firstName%></td>
            <td><%= admin.lastName  %></td>
            <td><%= admin.email %></td>
            <td><%= admin.role %></td>
            <td>

              <a href="/admin/byId/<%= admin._id %>">View Admin</a>
              <a href="/admin/<%= admin._id %>/edit">Edit</a>
             
              
             
              <button>
                <a href="/admin/<%= admin._id %>/delete">Delete</a>
                
              </button>
              

            

            </td>
          </tr>
          <% }); %>
        </tbody>
      </table>

      <div>
        <button>
          <a href="/admin/create">Create a New Admin</a>
        </button>
      </div>
     
      
      
</body>
</html>